  },
  "keywords": [
    "dashboard",
    "visualization",
    "operator"
  ],
  "author": "Jon Arild Nyg√•rd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jon-a-nygaard/operator-dashboard/issues"
  },
  "homepage": "https://github.com/jon-a-nygaard/operator-dashboard#readme",
  "dependencies": {
    "handlebars": "^4.2.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "prettier": "^1.18.2"
  }
}
