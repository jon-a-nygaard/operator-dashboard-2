  "description": "A learning project to visualize data in a dashboard application for operators",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run format && npm run build:handlebars",
    "build:handlebars": "node --experimental-modules build.js",
